package ListaExercicios;

import java.util.Scanner;

public class Exemplo1 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite o valor de X: ");
		int x = sc.nextInt();
		
		
		for (int i=1; i<x ; i++) {
			if (1 % 2 != 0) {
				System.out.println(i);
			}
		}
		
		
		sc.close();
	}

}
